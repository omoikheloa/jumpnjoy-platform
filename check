server {
    listen 80;
    server_name _;

    location /static/ {
        alias /home/your_username/myapp/yourrepo/backend/static/;
    }

    location /media/ {
        alias /home/your_username/myapp/yourrepo/backend/media/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/your_username/myapp/yourrepo/backend.sock;
    }
}
